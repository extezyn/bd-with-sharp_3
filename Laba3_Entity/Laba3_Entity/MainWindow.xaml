<Window x:Class="Laba3_Entity.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Laba3_Entity"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dg">
            <DataGrid.Columns>
                
            <DataGridTextColumn Binding="{Binding ClassDate}" Header="Дата:"/>
            <DataGridTextColumn Binding="{Binding Members.LastName}" Header="Фамилия посетителя:"/>
            <DataGridTextColumn Binding="{Binding Members.FirstName}" Header="Имя посетителя"/>
            <DataGridTextColumn Binding="{Binding Trainers.LastName}" Header="Фамилия тренера"/>
            <DataGridTextColumn  Binding="{Binding Trainers.FirstName}" Header="Фамилия тренера"/>
                
            </DataGrid.Columns>




        </DataGrid>
    </Grid>
</Window>
